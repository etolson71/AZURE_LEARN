				"typeProperties": {
					"location": {
						"type": "AzureBlobStorageLocation",
						"fileName": "stock_data_20240914.csv",
						"container": "stockdata"
					},
					"columnDelimiter": ",",
					"escapeChar": "\\",
					"firstRowAsHeader": true,
					"quoteChar": "\""
