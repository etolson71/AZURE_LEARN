			"name": "[concat(parameters('factoryName'), '/getcsv_load_db_tbl')]",
			"type": "Microsoft.DataFactory/factories/pipelines",
			"apiVersion": "2018-06-01",
			"properties": {
				"activities": [
					{
						"name": "Copy_rbx",
						"type": "Copy",
						"dependsOn": [],
						"policy": {
							"timeout": "0.12:00:00",
							"retry": 0,
							"retryIntervalInSeconds": 30,
							"secureOutput": false,
							"secureInput": false
						},
						"userProperties": [
							{
								"name": "Source",
								"value": "stockdata//stock_data_updated.csv"
							},
							{
								"name": "Destination",
								"value": "dbo.stockprices"
							}
						],
						"typeProperties": {
							"source": {
								"type": "DelimitedTextSource",
								"storeSettings": {
									"type": "AzureBlobStorageReadSettings",
									"recursive": true,
									"enablePartitionDiscovery": false
								},
								"formatSettings": {
									"type": "DelimitedTextReadSettings",
									"skipLineCount": 0
								}
							},
							"sink": {
								"type": "AzureSqlSink",
								"preCopyScript": "TRUNCATE TABLE dbo.stockprices",
								"writeBehavior": "insert",
								"sqlWriterUseTableLock": false,
								"disableMetricsCollection": false
							},
							"enableStaging": false,
							"validateDataConsistency": false,
							"translator": {
								"type": "TabularTranslator",
								"mappings": [
									{
										"source": {
											"name": "ticker",
											"type": "String",
											"physicalType": "String"
										},
										"sink": {
											"name": "ticker",
											"type": "String",
											"physicalType": "nvarchar"
										}
									},
									{
										"source": {
											"name": "ref_date",
											"type": "String",
											"physicalType": "String"
										},
										"sink": {
											"name": "ref_date",
											"type": "DateTime",
											"physicalType": "date"
										}
									},
									{
										"source": {
											"name": "price_open",
											"type": "String",
											"physicalType": "String"
										},
										"sink": {
											"name": "price_open",
											"type": "Double",
											"physicalType": "float"
										}
									},
									{
										"source": {
											"name": "price_high",
											"type": "String",
											"physicalType": "String"
										},
										"sink": {
											"name": "price_high",
											"type": "Double",
											"physicalType": "float"
										}
									},
									{
										"source": {
											"name": "price_low",
											"type": "String",
											"physicalType": "String"
										},
										"sink": {
											"name": "price_low",
											"type": "Double",
											"physicalType": "float"
										}
									},
									{
										"source": {
											"name": "price_close",
											"type": "String",
											"physicalType": "String"
										},
										"sink": {
											"name": "price_close",
											"type": "Double",
											"physicalType": "float"
										}
									},
									{
										"source": {
											"name": "volume",
											"type": "String",
											"physicalType": "String"
										},
										"sink": {
											"name": "volume",
											"type": "Double",
											"physicalType": "float"
										}
									}
								],
								"typeConversion": true,
								"typeConversionSettings": {
									"allowDataTruncation": true,
									"treatBooleanAsNumber": false
								}
							}
						},
						"inputs": [
							{
								"referenceName": "SourceDataset_rbx",
								"type": "DatasetReference",
								"parameters": {}
							}
						],
						"outputs": [
							{
								"referenceName": "DestinationDataset_rbx",
								"type": "DatasetReference",
								"parameters": {}
							}
						]
					}
				],
				"policy": {
					"elapsedTimeMetric": {}
				},
				"annotations": [],
